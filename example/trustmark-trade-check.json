{
  "openapi": "3.0.1",
  "info": {
    "title": "Trustmark.LAP.Trades.Api",
    "version": "v1"
  },
  "paths": {
    "/Hello": {
      "post": {
        "tags": [
          "Hello"
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/TradeCheck": {
      "post": {
        "tags": [
          "TradeCheck"
        ],
        "summary": "Perform a trade check for a TrustMark Licence Number (TMLN), optionally filtered by a schemeId. Provides a list of trades the licence is registered for.",
        "requestBody": {
          "description": "",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/TradeCheckRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/TradeCheckRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/TradeCheckRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "TradeCheckRequest": {
        "type": "object",
        "properties": {
          "publicId": {
            "type": "integer",
            "description": "The TMLN to query",
            "format": "int32"
          },
          "schemeId": {
            "type": "string",
            "description": "Optional schemeId filter, omit or set to view all trades under the TMLN",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "apiKey": {
        "type": "apiKey",
        "description": "Your x-api-key will be issued by TrustMark as part of onboarding",
        "name": "X-API-KEY",
        "in": "header"
      }
    }
  },
  "security": [
    {
      "apiKey": [ ]
    }
  ]
}